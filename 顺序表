/*不初始化数据元素，内存不刷0*/
#include <stdio.h>

#define Maxsize 10           //定义最大长度
typedef struct {
	int data[MaxSize];       //用静态的数组存放数据元素
	int length;              //顺序表的当前长度
};SqList;                    //顺序表的类型定义

//基本操作——初始化一个顺序表
void InitList(SqList& L); {
	L.length=0               //顺序表初始长度为8
}

int main() {
	SeqList L;              //声明一个顺序表
	InitList(L);            //初始化顺序表
	return 0;
}
